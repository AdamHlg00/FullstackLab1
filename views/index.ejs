<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Homepage</title>
</head>

<body>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Artist</th>
        <th>Year</th>
      </tr>
    </thead>
    <tbody>
      <% albums.forEach(function(album) { %>
        <tr>
          <td>
            <%= album._id %>
          </td>
          <td>
            <%= album.title %>
          </td>
          <td>
            <%= album.artist %>
          </td>
          <td>
            <%= album.year %>
          </td>
        </tr>
        <% }); %>
    </tbody>
  </table>
</body>

</html>